{
    "rules": {


        //Possible Errors
        "comma-dangle": [0, "always"],//closed:对象后面逗号,有bug:jsx里的{[]}会识别成对象
        "no-cond-assign": [2, "always"],
        "no-console": 0,
        "no-constant-condition": 1,
        "no-control-regex": 2,
        "no-debugger":1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 1,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 0,//closed:对jsx的外面包裹的圆括号判断的不准
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": [2, "both"],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 2,
        "valid-typeof": 2,



        //Best Practices
        "accessor-pairs": [2, {"getWithoutSet": true}],
        "block-scoped-var": 2,
        "complexity": [2, 20],
        "consistent-return": 2,
        "curly": 2,
        "default-case": 2,
        "dot-location": [2, "object"],
        "dot-notation": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-case-declarations": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-label": 2,
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native":2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-magic-numbers": 0,//closed:str.indexof的判断会直接使用-1
        "no-multi-spaces": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-param-reassign": [0, {"props": false}],//closed:有时候需要改
        "no-process-env": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-redeclare": [2, {"builtinGlobals": true}],
        "no-return-assign": [2, "always"],
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-void": 2,
        "no-warning-comments": 1,
        "no-with": 2,
        "radix": [2, "always"],
        "vars-on-top": 0,
        "wrap-iife": [2, "outside"],
        "yoda": 2,

        //Strict Mode
        "strict": [0, "global"], //closed:在module中声明为全局也会报错，说在module中是不必要的声明.


        //Variables
        "init-declarations": [0, "always"],//closed:有时候确实不需要声明
        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 2,
        "no-shadow": 2,
        "no-undef-init": 0,
        "no-undef": 2,
        "no-undefined": 2,
        "no-unused-vars": 0,//closed:React必须要声明否则会报错，但是代码中并用不到
        "no-use-before-define": 2,



        //node.js common.js
        "callback-return": 2,
        "global-require": 0,
        "handle-callback-err": 2,
        "no-mixed-requires": 2,
        "no-new-require": 2,
        "no-path-concat": 2,
        "no-process-exit": 2,
        "no-restricted-modules": [0,
            "assert","buffer","child_process","cluster","crypto","dgram","dns","domain","events","freelist","fs","http","https","module","net","os","path","punycode","querystring","readline","repl","smalloc","stream","string_decoder","sys","timers","tls","tracing","tty","url","util","vm","zlib"
        ],
        "no-sync": 2,




        //style
        "array-bracket-spacing": [2, "never"],
        "block-spacing": [2, "never"],
        "brace-style": 2,
        "camelcase": 2,
        "comma-spacing": [2, {"before": false, "after": true}],
        "comma-style": [2, "last"],
        "computed-property-spacing": [2, "never"],
        "consistent-this": [2, "self"],
        "eol-last": 2,
        "func-names": 2,
        "func-style": [2, "declaration"],
        "id-length": [2, {"min": 2, "max": 20, "properties": "always", "exceptions": ["x","y","e","i","j","k"]}],
        "id-match": [0, "^[a-z]+([A-Z][a-z]+)*$", {"properties": false}],
        "indent": [2, 4, {"SwitchCase": 1}],
        "jsx-quotes":[2,"prefer-single"],//single单引号比双引号好打
        "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
        "linebreak-style": [2, "unix"],
        "lines-around-comment": [2, { "beforeBlockComment": true, "beforeLineComment": true }],
        "max-depth": [2, 4],
        "max-len": [0, 80, 4],//closed:80还是太短了，有些字符串比较长
        "max-nested-callbacks": [2, 3],
        "max-params": [2, 3],
        "max-statements": [0, 10],//closed:对变量声明数量的限制，觉得不用限制
        "new-cap": 2,
        "new-parens": 2,
        "newline-after-var": [0, "always"],//closed:有时候并不需要空一行
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-continue": 2,
        "no-inline-comments": 0,//closed:有时候写在inline比较易读
        "no-lonely-if": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": [2, {"max": 2, "maxEOF": 1}],
        "no-negated-condition": 0,//closed:有时候需要取非
        "no-nested-ternary": 2,
        "no-new-object": 2,
        "no-plusplus": 2,
        "no-restricted-syntax": [0, "FunctionExpression", "WithStatement"],//closed:es6的方法定义会报错
        "no-spaced-func": 2,
        "no-ternary": 0,//closed:jsx中需要使用
        "no-trailing-spaces": [2, { "skipBlankLines": true }],//空行允许有空格
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 2,
        "object-curly-spacing": [0, "never"],//closed:对象的最后一个逗号，与comma-spacing冲突
        "one-var": [2, "never"],
        "operator-assignment": [2, "never"],
        "operator-linebreak": [0, "after"],//closed:jsx中使用条件表达式时候冒号最好独立一行
        "padded-blocks": [0, "always"],
        "quote-props": [2, "as-needed"],
        "quotes": [0, "single"],//closed:有时候字符串里有引号的时候，不能确定外层是什么引号
        "require-jsdoc": 2,
        "semi-spacing": [2, {"before": false, "after": true}],
        "semi": [2, "always"],
        "sort-vars": [0, { "ignoreCase": true }],
        "space-after-keywords": 2,
        "space-before-blocks": 2,
        "space-before-function-paren": [2,{"anonymous": "always", "named": "never"}],
        "space-before-keywords": [2, "always"],
        "space-in-parens": [2, "never"],
        "space-infix-ops": 2,
        "space-return-throw-case": 2,
        "space-unary-ops": [1, { "words": true, "nonwords": false }],
        "spaced-comment": [2, "always"],
        "wrap-regex": 2,





        //es6
        "arrow-body-style": [0, "as-needed"], //closed:箭头函数要根据简单还是复杂返回值来区分是否简写
        "arrow-parens": [2, "always"],
        "arrow-spacing": 2,
        "constructor-super": 2,
        "generator-star-spacing": [2, {"before": true, "after": false}],
        "no-arrow-condition": 2,
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-this-before-super": 2,
        "no-var": 2,
        "object-shorthand": 2,
        "prefer-arrow-callback": 2,
        "prefer-const": 0,//closed:const的意思应该是常量，没有修改的变量不一定是常量
        "prefer-reflect": 2,
        "prefer-spread": 2,
        "prefer-template": 2,
        "require-yield": 2

    },
    "env": {
        "es6": true //bug:es6的export default目前在rn中有bug，so现在暂时使用moudle.exports
        ,"node": true
    },
    "globals": {
        "fetch": false//false表示只读
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "arrowFunctions": true
    },
    "plugins": [
        "react"
    ]
}